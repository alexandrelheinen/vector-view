<?xml version="1.0"?>
<sdf version="1.4">
  <world name="default">
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <uri>model://sun</uri>
    </include>

    <include>
      <uri>model://icub_contact</uri>
      <pose>0 0 -.4 0 0 0</pose>
    </include>

    <model name="obstacle">
      <pose>0 0 0 0 0 0</pose>
      <link name='box'>
        <pose>.45 0 1 0 0 0</pose>
        <inertial>
          <mass>1000</mass>
        </inertial>
        <collision name ='collision'>
          <geometry>
            <box><size>.2 .8 1.1</size></box>
          </geometry>
        </collision>
        <visual name ='visual'>
          <geometry>
            <box><size>.2 .8 1.1</size></box>
          </geometry>
        </visual>
      </link>

      <joint name="fixed to ground" type="revolute">
      <parent>box</parent>
      <child>ground_plane::link</child>
       <axis>
        <limit>
          <lower>-.05</lower>
          <upper>0.05</upper>
        </limit>
        <xyz>0 0 1</xyz>
        </axis>
      </joint>

    </model>

  </world>
</sdf>
